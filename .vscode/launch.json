{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart"
        },
        {
            "name": "recipe_finder",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "recipe_finder (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "recipe_finder (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "recipe_finder (web)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--flavor",
                "dev",
                "--web-port=8100",
                "--web-renderer=canvaskit",
                "--dart-define=FLUTTER_WEB_USE_SKIA=true"
            ],
            "deviceId": "chrome",
            "env": {
                "FLUTTER_WEB": "true"
            }
        },
    ]
}